            <%- body %> <!-- This is where page-specific content will be injected -->
        </main>
        <div class="small-2 cell">
        <aside class="dashboard-sidebar">
            <%- include('../partials/right-sidebar') %> <!-- Include right sidebar here -->
          </aside>
        </div>
  </div>
  </div>

  <%- include('../partials/footer') %>

  <script src="/js/main.js"></script> 
  <!-- Add other JS files -->
