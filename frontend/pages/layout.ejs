<body>
  <%- include('../partials/header') %>

  <div class="grid-container neg-margin">
    <div class="grid-x">
        <div class="small-2 cell">
            <aside class="dashboard-sidebar">
                <%- include('../partials/sidebar') %>
            </aside>
        </div>

